        <alwaysRun>0</alwaysRun>
        <broadcast>0</broadcast>
        <command>use strict;&#10;use warnings;&#10;&#10;use ElectricCommander;&#10;&#10;&#10;my $cmdr = ElectricCommander-&gt;new({format =&gt; "json"});&#10;&#10;# parameters&#10;my $resource = "$[resource]";&#10;my $force_build = "$[forceBuilds]";&#10;my $runStaticAnalysis = "$[runStaticAnalysis]";&#10;my $url_list = q($[cartypes]);&#10;&#10;# list of job ids that this script invokes&#10;my @job_ids = ();&#10;&#10;my $repository_root = "https://svn.bose.com/asd";&#10;my $nightly_procedure = "NightlyBuildCartype";&#10;&#10;my @urls = split("\n", $url_list);&#10;&#10;foreach my $url (@urls) {&#10;	$url =~ s/^\s+|\s+$//g;&#10;	$url =~ s/^${repository_root}\///;&#10;	&#10;	# remaining part of url should be UA_CARS/project/cartype/...&#10;	next if ($url !~ /^UA_CARS/);&#10;	my @path = split('/', $url);&#10;	my ($project, $cartype) = ($path[1], $path[2]);&#10;	my $branch = "";&#10;	if ($path[3] eq "trunk") {&#10;		$branch = "HEAD";&#10;	} else {&#10;		$branch = $path[4];&#10;	}&#10;	&#10;	my $job_id = $cmdr-&gt;runProcedure("ASD_${project}", &#10;		{&#10;			procedureName =&gt; "$nightly_procedure",&#10;			actualParameter =&gt;&#10;			[&#10;				{ actualParameterName =&gt; "cartype", value =&gt; $cartype },&#10;				{ actualParameterName =&gt; "branch", value =&gt; $branch },&#10;				{ actualParameterName =&gt; "forceBuild", value =&gt; $force_build },&#10;				{ actualParameterName =&gt; "buildResource", value =&gt; $resource },&#10;                { actualParameterName =&gt; "runStaticAnalysis", value =&gt; $runStaticAnalysis },&#10;			]&#10;		}&#10;	);&#10;	# { actualParameterName =&gt; "runLint", value =&gt; "false" },&#10;	push(@job_ids, $job_id-&gt;{"responses"}[0]{"jobId"});&#10;}&#10;&#10;$cmdr-&gt;setProperty("/myJob/jobIds", join("\n", @job_ids));&#10;&#10;print "Job IDs:\n";&#10;print join("\n", @job_ids) . "\n";</command>
